{
    "compilerOptions": {
      "allowImportingTsExtensions": true,
      "allowJs": true,
      "esModuleInterop": true,
      "jsx": "react-jsx",
      "lib": ["DOM", "ESNext"],
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "strict": true,
      "skipLibCheck": true,
      "noImplicitAny": false
    },
    "include": ["src"]
  }
  